<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- Sequences for project number -->
        <record id="project_sequence" model="ir.sequence.type">
            <field name="name">Project Name</field>
            <field name="code">project.project</field>
        </record>

        <record id="seq_ids_project" model="ir.sequence">
            <field name="name">Project Name</field>
            <field name="code">project.project</field>
            <field name="prefix">IDEAS%(year)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
